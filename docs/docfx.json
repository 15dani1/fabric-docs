{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/trident/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None"
    },
    "fileMetadata": {
      "ms.service": {
        "docs/data-explorer/**/*.md": "azure",
        "docs/data-science/**/*.md": "azure",
        "docs/data-engineering/**/*.md": "azure",
        "docs/data-warehousing/**/*.md": "azure",
        "docs/data-factory/**/*.md": "azure",
        "docs/power-bi/**/*.md": "azure"
      }
    },
    "template": [],
    "dest": "trident-docs"
  }
}