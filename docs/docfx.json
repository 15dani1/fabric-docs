{
    "build": {
        "content": [
            {
                "src": "./",
                "files": [
                    "**/*.md",
                    "**/*.yml",
                    "**/*.svg"
                ],
                "exclude": [
                    "**/obj/**",
                    "**/includes/**",
                    "_themes/**",
                    "_themes.pdf/**",
